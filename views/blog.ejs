							<div class=" form-group">
								<input type="text" class="form-control"  name="number_sizes" placeholder="number of size" id="box"  >
							</div>

							
							</div>
							<% if(items.length != 0) { %>
								<% for (var i =0; i <items.length;  i++ ) { %>
									<% for (var g =0; g <sizes.length;  g++ ) { %>
										<% if(items[i].size === sizes[g]._id) { %>
							<ul class="list-group list-group-flush">
								<li class="list-group-item"> <%= sizes[i].size %> 
				
								</li>
								
							  </ul>
							  <% } } } }%>
                       <div class="col-md-12 form-group">
								<button type="sumbit" value="button" class="button button-login w-100" formaction="/add_number_of_size" >Add new Size</button>
								
							</div>
						
						<div class="col-lg-12">
